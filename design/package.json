  "private": true,
  "scripts": {
    "dev": "cross-env webpack-dev-server --open --hot --inline --host --port 80 --config ./build/webpack.dev.conf.js",
    "unit": "cross-env webpack-dev-server --open --hot --config ./build/webpack.dev.conf.js",
    "mock": "json-server --watch ./mock/mock-index.js --routes ./mock/routes.json",
    "build-dll": "webpack --config ./build/webpack.dll.conf.js",
