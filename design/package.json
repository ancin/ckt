    "ss-messsage": "1.0.4",
    "ss-waterfall": "1.0.4",
    "vue": "2.4.4",
    "vue-router": "2.8.1",
    "vuex": "2.5.0"
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "6.0.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-env": "1.6.1",
    "cross-env": "3.0.0",
    "css-loader": "0.25.0",
    "eslint": "4.11.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.9.0",
